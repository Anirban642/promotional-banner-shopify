{
  "version": 2,
  "framework": "remix",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "app/entry.server.tsx": {
      "includeFiles": "build/**"
    }
  }
}